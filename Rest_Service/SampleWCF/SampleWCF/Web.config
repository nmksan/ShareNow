          <webHttp />
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <services>
      <service name="SampleWCF.Sampleservice"   behaviorConfiguration="ServiceBehavior">
        
        <!--<endpoint binding="webhttpbinding" contract="samplewcf.isampleservice" behaviorconfiguration="web"></endpoint>-->
        <endpoint binding="webHttpBinding" contract="SampleWCF.ISampleservice" behaviorConfiguration="web">
        </endpoint>
      </service>
    </services>
    <protocolMapping>
      <add binding="basicHttpsBinding" scheme="https" />
    </protocolMapping>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
